{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    // Compile the ts
    {
      "type": "node",
      "request": "launch",
      "name": "Compile Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/Bot/node_modules/typescript/bin/tsc",
      "args": ["-p", "${workspaceFolder}/Bot/tsconfig.json"],
      "outFiles": ["${workspaceFolder}/Bot/dist/**/*.js"]
    },
    // rest the commands
    {
      "type": "node",
      "request": "launch",
      "name": "Rest Commands",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/Bot/rest.js"
    },
    // launch index.js
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/Bot/dist/index.js",
      "preLaunchTask": "Compile Program"
    },
    // Lauch Bot dev
    {
      "name": "Launch Bot Dev",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/Bot"
    }
  ]
}
